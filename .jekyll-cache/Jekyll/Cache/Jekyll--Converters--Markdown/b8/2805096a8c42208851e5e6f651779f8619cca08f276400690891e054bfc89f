        <span class="c1">//equals</span>
        <span class="n">list</span><span class="o">.</span><span class="na">forEach</span><span class="o">(</span><span class="nc">System</span><span class="o">.</span><span class="na">out</span><span class="o">::</span><span class="n">println</span><span class="o">);</span>
    <span class="o">}</span>

    <span class="kd">private</span> <span class="kt">void</span> <span class="nf">strStream</span><span class="o">(){</span>
        <span class="nc">IntStream</span> <span class="n">streamA</span> <span class="o">=</span> <span class="s">"12345_abcdefg"</span><span class="o">.</span><span class="na">chars</span><span class="o">();</span>
        <span class="n">streamA</span><span class="o">.</span><span class="na">forEach</span><span class="o">(</span><span class="nc">System</span><span class="o">.</span><span class="na">out</span><span class="o">::</span><span class="n">println</span><span class="o">);</span>

