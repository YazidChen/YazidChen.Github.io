    <span class="o">}</span>

    <span class="kd">private</span> <span class="kt">void</span> <span class="nf">streamGenerate</span><span class="o">()</span> <span class="o">{</span>
        <span class="nc">Stream</span><span class="o">&lt;</span><span class="nc">Date</span><span class="o">&gt;</span> <span class="n">stream</span> <span class="o">=</span> <span class="nc">Stream</span><span class="o">.</span><span class="na">generate</span><span class="o">(</span><span class="nl">Date:</span><span class="o">:</span><span class="k">new</span><span class="o">).</span><span class="na">limit</span><span class="o">(</span><span class="mi">10</span><span class="o">);</span>
        <span class="n">stream</span><span class="o">.</span><span class="na">forEach</span><span class="o">(</span><span class="nc">System</span><span class="o">.</span><span class="na">out</span><span class="o">::</span><span class="n">println</span><span class="o">);</span>
    <span class="o">}</span>

